names=["Alice","Bob","Anna","David","Amy","Andrew"]

names_starts_with_A = [name for name in names if name.startswith("A")]

print(names_starts_with_A)

nums=[2,4,6,8,10,7,5,3]

odd_nums=[num for num in nums if num%2!=0]
print(odd_nums)

list1=[1,2,3,4,5]
list2=[6,7,8,9,10]

pairs=[[x,y] for x in list1 for y in list2 if x+y>10]
print(pairs)

